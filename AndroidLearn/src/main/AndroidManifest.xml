<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.themelove.androidlearn">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:name=".AppBase.TLApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppDefault">
        <activity
            android:name=".main.MainActivity"
            android:theme="@style/AppDefault">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".Case.appbar.UI.ToolBarActivity"
            android:theme="@style/AppDefault" />
        <activity android:name=".Case.progressbar.ProgressBarTestActivity"
            android:theme="@style/AppDefault">
        </activity>
        <activity android:name=".media.AudioActivity"
            android:theme="@style/AppDefault">
        </activity>
        <activity android:name=".Case.PagerTab.PagerTabStripActivity"/>


        <activity android:name=".Case.TabLayout.TabLayoutActivity"></activity>

        <meta-data
            android:name="ThemeLove"
            android:value="${ThemeLoveChannel}" />
    </application>


</manifest>